
@{
    ViewBag.Title = "Groupe";
}

@if (Model.Count == 0)
{
    <h2>Vous n'avez pas encore rejoint de groupe.</h2>
}
else
{
    <table>
        <tr>
            <th>Nom</th>
            @foreach (Xmas.Models.GroupeInfo groupe in Model)
            {
                <td>@groupe.Nom</td>
            }
        </tr>
        <tr>
            <th>Description</th>
            @foreach (Xmas.Models.GroupeInfo groupe in Model)
            {
                <td>@groupe.Description</td>
            }
        </tr>

    </table>
}
